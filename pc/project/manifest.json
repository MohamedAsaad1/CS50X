{
    "name": "Dark",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "change your page to Drak mode:) ",
    "background": {
        "scripts": [
            "popup.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "DarkMode",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
   
}